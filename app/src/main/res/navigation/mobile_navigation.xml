<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.my_test6.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_blink"
        android:name="com.example.my_test6.ui.blink.BlinkFragment"
        android:label="@string/title_blink"
        tools:layout="@layout/fragment_blink" />

    <fragment
        android:id="@+id/navigation_question"
        android:name="com.example.my_test6.ui.question.QuestionFragment"
        android:label="@string/title_question"
        tools:layout="@layout/fragment_question" />
    <fragment
        android:id="@+id/navigation_edu"
        android:name="com.example.my_test6.ui.edu.EduFragment"
        android:label="@string/title_edu"
        tools:layout="@layout/fragment_edu" />
    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.my_test6.ui.user.UserFragment"
        android:label="@string/title_user"
        tools:layout="@layout/fragment_user" />
</navigation>
